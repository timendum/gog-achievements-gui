{
  "name": "gog-achievements-gui",
  "version": "0.0.1",
  "description": "A gog achievements manager built with React, Tailwind and Vite",
  "private": true,
  "license": "AGPL-3.0-only",
  "author": "Timendum",
  "scripts": {
    "start": "vite build && electrobun dev",
    "dev": "electrobun dev --watch",
    "dev:hmr": "concurrently \"bun run hmr\" \"bun run start\"",
    "hmr": "vite --port 5173",
    "build:canary": "vite build && electrobun build --env=canary"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.2.1",
    "electrobun": "^1.14.4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.2.1",
    "winreg": "^1.2.5"
  },
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/winreg": "^1.2.36",
    "@vitejs/plugin-react": "^5.1.4",
    "babel-plugin-react-compiler": "^1.0.0",
    "concurrently": "^9.2.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
